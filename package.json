{
  "name": "the-obsidian-plot",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "deno run build && astro dev",
    "build": "deno run build-content && deno run build-search-index && astro build",
    "preview": "astro preview",
    "deploy": "gh-pages -d dist",
    "astro": "astro",
    "format": "prettier --write \"{src/**,}/*.{ts,astro}\"",
    "build-content": "deno run --allow-read=./content --allow-write=./content ./src/scripts/normalize-obsidian-vault-content.ts",
    "build-search-index": "deno run --allow-read=./content --allow-write=./public ./src/scripts/generate-search-index.ts"
  },
  "dependencies": {
    "@astrojs/image": "^0.18.0",
    "@astrojs/mdx": "^4.3.4",
    "@astrojs/react": "^4.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "astro": "^5.13.5",
    "fuse.js": "^7.1.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}
